# Last updated: 18.08.2025, 11:56:23
class Solution:
    def subsets(self, nums: List[int]) -> List[List[int]]:
        ans = []

        def dfs(i, s):

            ans.append(s[:])

            for j in range(i, len(nums)):
                s.append(nums[j])

                dfs(j+1, s)

                s.pop()
        
        dfs(0, [])
        return ans